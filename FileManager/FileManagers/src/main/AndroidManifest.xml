<manifest
    package="com.project.filemanager"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <!--读取资源-->
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
        android:name="com.file.chooser.activity.IApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.file.chooser.activity.FileChooserActivityStart"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.file.chooser.lib.FileChooserActivity">
        </activity>
    </application>

</manifest>
